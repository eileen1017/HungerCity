# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'HungerCity' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for HungerCity

  pod 'FacebookLogin'
  pod 'FacebookCore'
  pod 'FacebookSDK'
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'Firebase/Storage'
  pod 'JGProgressHUD'
  pod 'SwiftyJSON'
end
